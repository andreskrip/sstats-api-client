{
  "name": "andreskrip/sstats-api-client",
  "description": "A simple PHP client for the sstats.net API using Guzzle",
  "keywords": [
    "sstats",
    "api-client",
    "football-api"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Skripnik",
      "email": "andreskrip@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Andreskrip\\SstatsApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Andreskrip\\SstatsApiClient\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
